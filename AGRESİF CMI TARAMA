GRP_LABELS = "Label Ayarları"
label_text_size = input.string("normal", "Label Yazı Boyutu", options = ["tiny", "small", "normal", "large", "huge"], group = GRP_LABELS, tooltip = "Etiketlerin metin boyutunu ayarlar.")


// Önceki etiketi sil
label.delete(lab_1[1])
label.delete(lab_2[1])
